{
  "release-type": "rust",
  "include-component-in-tag": false,
  "plugins": [
    "cargo-workspace"
  ],
  "exclude-paths": [
    ".github/workflows"
  ],
  "packages": {
    "binix": {},
    "binix-server": {},
    "binix-client": {},
    "binix-token": {}
  },
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "perf",
      "section": "Performance Improvements"
    },
    {
      "type": "deps",
      "section": "Dependencies"
    },
    {
      "type": "revert",
      "section": "Reverts"
    },
    {
      "type": "docs",
      "section": "Documentation"
    },
    {
      "type": "style",
      "section": "Styles"
    },
    {
      "type": "chore",
      "section": "Miscellaneous Chores"
    },
    {
      "type": "refactor",
      "section": "Code Refactoring"
    },
    {
      "type": "test",
      "section": "Tests"
    },
    {
      "type": "build",
      "section": "Build System"
    },
    {
      "type": "ci",
      "section": "Continuous Integration"
    }
  ]
}