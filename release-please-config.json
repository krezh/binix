{
  "group-pull-request-title-pattern": "chore: release",
  "include-v-in-tag": false,
  "include-component-in-tag": false,
  "release-type": "rust",
  "exclude-paths": [
    ".github/workflows"
  ],
  "packages": {
    "binix": {
      "component": "binix"
    },
    "client": {
      "component": "binix-client"
    },
    "server": {
      "component": "binix-server"
    },
    "token": {
      "component": "binix-token"
    }
  },
  "plugins": [
    {
      "type": "cargo-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "binix",
      "components": [
        "binix",
        "binix-client",
        "binix-server",
        "binix-token"
      ]
    }
  ],
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "perf",
      "section": "Performance Improvements"
    },
    {
      "type": "deps",
      "section": "Dependencies"
    },
    {
      "type": "revert",
      "section": "Reverts"
    },
    {
      "type": "docs",
      "section": "Documentation"
    },
    {
      "type": "style",
      "section": "Styles"
    },
    {
      "type": "chore",
      "section": "Miscellaneous Chores"
    },
    {
      "type": "refactor",
      "section": "Code Refactoring"
    },
    {
      "type": "test",
      "section": "Tests"
    },
    {
      "type": "build",
      "section": "Build System"
    },
    {
      "type": "ci",
      "section": "Continuous Integration"
    }
  ]
}